{
    "store": {
        "version": "1.0.0",
        "plugins": [
            {
                "name": "oras"
            }
        ]
    },
    "policy": {
        "version": "1.0.0",
        "plugin": {
            "name": "configPolicy",
            "artifactVerificationPolicies": {
                "application/vnd.ratify.spdx.v0": "all"
            }
        }
    },
    "verifier": {
        "version": "1.0.0",
        "plugins": [
            {
                "name": "licensechecker",
                "artifactTypes": ["application/vnd.ratify.spdx.v0"],
                "allowedLicenses": [
                    "MIT"
                ]
            }
        ]
    }
}